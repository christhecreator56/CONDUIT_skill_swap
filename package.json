{
  "name": "conduit-skill-exchange",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "db:generate": "dotenv -e .env -- drizzle-kit generate",
    "db:migrate": "dotenv -e .env -- drizzle-kit migrate",
    "db:studio": "dotenv -e .env -- drizzle-kit studio",
    "db:push": "dotenv -e .env -- drizzle-kit push",
    "db:init": "dotenv -e .env -- tsx scripts/init-db.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.0.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.2",
    "framer-motion": "^12.23.3",
    "lucide-react": "^0.263.1",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-hot-toast": "^2.4.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.8.1",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.15.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.0.2",
    "vite": "^7.0.4"
  }
}
